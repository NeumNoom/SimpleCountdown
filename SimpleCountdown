count = 20
while count > -1:
    if count == 20:
        print("Starting count....")
        print(count)  # Print the current count
        count -= 1  # Decrement count by 1

    if count > 10:
        print("Still lowering, homie")
        while count > 10:
            print(count)
            count -= 1  # Decrement count inside the while loop

    elif count == 10:
        print("I've done my part")
        print("lets continue")
        print("Even lower!")
        while count == 10:
            print(count)
            count -= 1  # Decrement count inside the while loop

    elif 0 < count <= 10:
        while 0 < count <= 10:
            print(count)
            count -= 1  # Decrement count inside the while loop

    else:
        print("All done!")
        break
